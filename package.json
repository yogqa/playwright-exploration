{
  "name": "playwright-exploration",
  "version": "1.0.0",
  "description": "Antigravity Test Automation Framework â€” AI-embedded Playwright + TypeScript",
  "scripts": {
    "pretest": "node scripts/clean-reports.js",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"*.ts\"",
    "typecheck": "tsc --noEmit",
    "allure:generate": "npx allure generate allure-results --clean -o allure-report",
    "allure:open": "npx allure open allure-report",
    "mcp": "npx -y @playwright/mcp --browser chromium --storage-state ./auth/admin.json",
    "agent-cli": "npx -y @playwright/cli --browser chromium",
    "agent-cli:auth": "npx -y @playwright/cli --browser chromium --storage-state ./auth/admin.json"
  },
  "keywords": [
    "playwright",
    "test-automation",
    "typescript",
    "antigravity"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "allure-commandline": "^2.36.0",
    "allure-playwright": "^3.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "winston": "^3.17.0",
    "zod": "^3.25.17"
  }
}